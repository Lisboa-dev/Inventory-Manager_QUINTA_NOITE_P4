model Lote{
    id            Int @id @default(autoincrement())
    usuario       User @relation("LotesToUser", fields: [usuarioId], references: [id])
    usuarioId     Int

    genre         Genero @relation("LotesToGenre", fields: [genreId], references: [id])
    genreId       Int

    produtos Produto[] @relation("ProdutosToLote")

    createdAt DateTime @default(now())
}