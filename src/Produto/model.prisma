model Produto {
  id                Int      @id @default(autoincrement())
  nome              String
  descricao         String? 
  preco             Decimal   @db.Decimal(10, 2)
  quantidadeEstoque Int       @default(0)
  lotes             Lote[]
  createdAt         DateTime  @default(now())
  updatedAt         DateTime  @updatedAt
}