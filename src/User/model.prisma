model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique  
  senha    String 

  generos Genero[] @relation("GenerosToUser")
  produtos Produto[] @relation("ProdutosToUser")
  lotes Lote[] @relation("LotesToUser")
  createdAt DateTime @default(now())
}